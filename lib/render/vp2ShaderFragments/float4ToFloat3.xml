<fragment uiName="float4ToFloat3" name="float4ToFloat3" type="plumbing" class="ShadeFragment" version="1.0" feature_level="0">
    <description>
        Extracts the XYZ component of a 4D float vector.
    </description>
    <properties>
        <float4 name="input"/>
    </properties>
    <values/>
    <outputs>
